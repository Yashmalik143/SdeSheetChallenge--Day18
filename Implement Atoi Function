int atoi(string str) {
    // Write your code here.
    int ans=0;
    int sign=1;
    int i = 0;
    if(str[i]=='-')
    {
        sign = -1;
        i++;
    }
    while(str[i])
    {
        if(str[i]-'0'>=0 && str[i]-'0'<=9)
        {
           ans=ans*10+str[i]-'0';
          
        }
          i++;
        
    } return ans*sign;

}
